@section Styles {
    <!-- Thêm CSS riêng của trang giới thiệu vào đây -->
    <link href="https://fonts.googleapis.com/css2?family=Roboto&display=swap" rel="stylesheet">
    <link type="text/css" rel="stylesheet" href="https://odoocdn.com/web/assets/1/c23d572/web.assets_frontend.min.css" />
    <style>
        /* Thống nhất font chữ toàn trang */
        body, h1, h2, h3, h4, h5, h6, p, a, span, div, small, button {
            font-family: "Roboto", "Arial", sans-serif !important;
            font-style: normal !important;
        }

        /* Ghi đè một vài style để khớp với layout mới */
        main {
            padding-top: 0;
        }

        .footer {
            display: none;
        }
    </style>
}

